@font-face {
  font-family: 'TrimPosterWebCompressed';
  src: url("{{ 'TrimPosterWebCompressed.woff' | asset_url }}") format('woff')
  , url("{{ 'TrimPosterWebCompressed.ttf' | asset_url }}") format('truetype')
  , url("{{ 'TrimPosterWebCompressed.otf' | asset_url }}") format('opentype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'NanumPenScript';
  src: url("{{ 'NanumPenScript-Regular.ttf' | asset_url }}") format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'Raleway-Thin';
  src: url("{{ 'Raleway-Thin.ttf' | asset_url }}") format('truetype');
}

@font-face {
  font-family: 'Raleway-Bold';
  src: url("{{ 'Raleway-Bold.ttf' | asset_url }}") format('truetype');
}

@font-face {
  font-family: 'Raleway-ExtraBold';
  src: url("{{ 'Raleway-ExtraBold.ttf' | asset_url }}") format('truetype');
}

h2.page-title-wb  {
	font-family: 'TrimPosterWebCompressed', sans-serif;
	font-weight: 400;
	margin: 0 0 20px 0;
    font-size: 25vw;
    line-height:19vw;
    transition: font-size 0.3s ease;
    @media only screen and (min-width: 600px) {
        margin: 10px 0;
        font-size: 20vw;
        line-height: 15vw;
        transition: font-size 0.3s ease;
      }
      @media only screen and (min-width: 1024px) {
        font-size: 213px;
        line-height: 160px;
        transition: font-size 0.3s ease;
        margin: 20px 0 40px 0;
      }
}

.find-hero-banner-title {
    width: 100%;
    display:inline-grid;
    @media only screen and (min-width: 764px) {

        /*display on one line not break over 2*/
        display: flex;
        justify-content: space-around; 
        align-items: center;
        flex-wrap: wrap;
      }
  }

