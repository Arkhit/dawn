{% for block in section.blocks %}
<h2>{{ block.settings.question }}
<p> {{ block.settings.answer }} </p>

{% endfor %}

{% schema %}
{
  "name": "FAQs",
  "tag": "section",
  "class": "faqs",
  "blocks": [
    {
      "name": "Questions",
      "type": "questions",
      "settings": [
        {
        "type":"text",
        "id":"question",
        "label": "Question"
        },
        {
          "type":"text",
          "id":"answer",
          "label": "Answer"
        }
      ]
    }
  ]
}
{% endschema %}
