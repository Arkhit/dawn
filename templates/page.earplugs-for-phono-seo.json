    "76712172-7c2c-4e4f-b522-130f1cc7f4ff": {
      "type": "loop-article-img-txt",
      "blocks": {
        "template--15748392779972__76712172-7c2c-4e4f-b522-130f1cc7f4ff-1667392897d6806ce4-0": {
          "type": "heading",
          "settings": {
            "heading": "Start living life at your volume",
            "heading_size": "h2"
          }
        },
        "template--15748392779972__76712172-7c2c-4e4f-b522-130f1cc7f4ff-1667392897d6806ce4-1": {
          "type": "text",
