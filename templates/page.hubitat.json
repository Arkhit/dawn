    "09db0114-b227-4d20-a4bf-866ebb673289": {
      "type": "image-with-copy",
      "settings": {
        "background_color": "white",
        "image": "shopify:\/\/shop_images\/Hubitat_HubAnimation_FINAL_5.gif",
        "heading": "Hubitat works locally",
        "subheading": "No internet? No problem",
        "text": "Hubitat is a local smart home platform. The <b>required<\/b> <a href=\"https:\/\/hubitat.com\/products\" target=\"_blank\" rel=\"noopener noreferrer\">Hubitat hub<\/a> is connected directly to your router (either by WiFi or wired Ethernet), which creates a home-based network that works even without an internet connection. Thanks to the hub, there’s also less latency due to data processing within your network and <a href=\"https:\/\/konnected.io\/blogs\/smart-home-security\/cloud-vs-local-what-smart-home-platform-is-best-for-you-and-why\" target=\"_blank\" rel=\"noopener noreferrer\">not the cloud.<\/a>"
      }
    },
