    "16581501774a32acf8": {
      "type": "loop-article-img-txt",
      "blocks": {
        "15d54bfe-1b48-43c0-9fad-0527a7874216": {
          "type": "heading",
          "settings": {
            "heading": "How does a doctor diagnose tinnitus?",
            "heading_size": "h2"
          }
        },
        "16581501770ad7ecad-1": {
          "type": "text",
          "settings": {
            "text": "<p>If you think you might have tinnitus, it’s important to see your doctor or a hearing specialist such as an audiologist or ear, nose and throat specialist (ENT).<\/p>",
            "text_style": "body"
          }
        }
      },
      "block_order": [
        "15d54bfe-1b48-43c0-9fad-0527a7874216",
        "16581501770ad7ecad-1"
      ],
      "settings": {
