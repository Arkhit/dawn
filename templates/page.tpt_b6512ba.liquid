        }
    } else {
      if (!targetElement) {
          window.__tpt_everlasting_tickrate[slt] = ( window.__tpt_everlasting_tickrate[slt] || 0 )+1;
          if (window.__tpt_everlasting_tickrate[slt] < 5){
