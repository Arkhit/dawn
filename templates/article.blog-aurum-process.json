    "166046188908e08637": {
      "type": "rich-text",
      "blocks": {
        "166046188882d65dad-0": {
          "type": "heading",
          "settings": {
            "heading": "AI earplugs, done. What’s next?",
            "heading_size": "h2"
          }
        },
        "92c1d27d-1a60-4fbe-b18b-18e6ec3ea987": {
          "type": "text",
          "settings": {
            "text": "<p>The release of the final set of Loop Aurum is currently underway – which means the design team is already pitching ideas for the new year.<\/p>",
            "description_padding": 32
          }
        }
      },
      "block_order": [
        "166046188882d65dad-0",
