    "a9b4069e-506e-4604-be73-2a7162ee703b": {
      "type": "rich-text",
      "blocks": {
        "4c730c8d-3e8d-4fdc-ac81-0237f604cb00": {
          "type": "heading",
          "settings": {
            "heading": "<h3>4. Remember an eye mask<\/h3>",
            "heading_size": "h2"
          }
        },
        "22c2794f-ecaa-4629-808b-d37a3c92aae6": {
          "type": "text",
          "settings": {
            "text": "<p><strong>An eye mask will help to keep you fast asleep<\/strong> as the sun starts streaming through your tent early in the morning. Things don’t typically get going at festivals until around noon, so with the help of an eye mask, you can sleep in for longer and not have to miss out on any of the action.<\/p>",
            "description_padding": 32
          }
        }
