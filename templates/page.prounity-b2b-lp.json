    "ea105b2c-5706-4917-8d4d-2eeb78f7a518": {
      "type": "image-with-text",
      "blocks": {
        "template--15830469869764__ea105b2c-5706-4917-8d4d-2eeb78f7a518-1675773179e6de8e1f-0": {
