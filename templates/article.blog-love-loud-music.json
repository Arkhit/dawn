    "1656049353eec351e4": {
      "type": "loop-article-richtext",
      "blocks": {
        "aa305c36-859d-4b37-ba71-7b5e8e36dd26": {
          "type": "heading",
          "settings": {
            "heading": "5 reasons we love loud music",
            "heading_size": "h1"
          }
        }
      },
      "block_order": [
        "aa305c36-859d-4b37-ba71-7b5e8e36dd26"
      ],
      "settings": {
        "desktop_content_position": "center",
