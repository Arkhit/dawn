    "who-its-for": {
      "type": "two-column",
      "settings": {
        "show_related": true,
        "image": "shopify:\/\/shop_images\/ALARM_PANEL_PRO_WITH_INTERFACE_MODULES_LIFESTYLE.png",
        "heading": "CHOOSE THE ALARM PANEL PRO INTERFACE KIT IF YOU:",
        "text": "Still want to use your traditional alarm system and just add smart home functionality to it \nWant to keep your professional home monitoring (if you subscribe to one)\nNeed Ethernet connectivity & POE because your home is pre-wired for Ethernet*\nWant to have 12 zones and 2 siren\/strobe outputs per panel*\nWant to ease your way into smart home alarms before fully switching over\nLove using your existing keypad and want to try using smart home apps with it to control the system\n*Compare with <u><a href=\"https:\/\/konnected.io\/collections\/shop-now\/products\/konnected-alarm-panel-interface-kit\" target=\"_blank\">Alarm Panel<\/a><\/u>"
      }
    },
    "reasons-to-love": {
      "type": "two-column-blue",
      "settings": {
        "show_related": true,
        "image": "shopify:\/\/shop_images\/Page_4_REASONS_TO_LOVE_KONNECTED.png",
        "heading": "REASONS TO LOVE KONNECTED FOR APP-BASED HOME MONITORING",
        "text": "Works with leading smart home platforms & voice assistants\nInfinitely expandable by adding any of our alarm panels\nNo additional monthly fees or subscriptions \nReal-time notifications so you can monitor your home from anywhere\nAutomatically arms\/disarms your home\nChoose your favorite smart home app\nUses your pre-existing wired sensors and siren\nCreate smart home routines & automations\nWorks with nearly any wired alarm system including ADT, Honeywell, and many more"
      }
    },
