    "c1914074-e112-47ac-8be5-71e50691e7b4": {
      "type": "rich-text",
      "blocks": {
        "template--15826134302916__c1914074-e112-47ac-8be5-71e50691e7b4-16752485096357b5bf-1": {
          "type": "text",
          "settings": {
            "text": "<p>Everybody has their own opinions on the most annoying sounds in the world. Most people hate the wet smacking sounds that come with open-mouth chewing. Others are guilty of it themselves.<\/p>",
            "description_padding": 32
          }
        }
      },
      "block_order": [
