      }
    },
    "eacd8309-2308-416c-a517-4a1265e97950": {
      "type": "loop-article-img-txt",
      "blocks": {
        "template--15748392779972__eacd8309-2308-416c-a517-4a1265e97950-1667391081964b5b95-1": {
          "type": "text",
          "settings": {
            "text": "<p>° Anxiety and stress<\/p><p>° Feeling triggered and overwhelmed to the point you need to leave the room.<\/p><p>° A harder time concentrating and communicating with others.<\/p><p>° Difficulty with decision-making and processing information in the moment.<\/p><p>Luckily, Loop earplugs are<br\/>here to help.<\/p>",
            "text_style": "body"
          }
        }
      },
      "block_order": [
        "template--15748392779972__eacd8309-2308-416c-a517-4a1265e97950-1667391081964b5b95-1"
      ],
      "settings": {
